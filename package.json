{
  "name": "cricket",
  "version": "1.0.3",
  "description": "Cricket application",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "webpack": "webpack --watch --config webpack.dev.js",
    "clean": "find ./public/app/ -name '*.js*' -delete",
    "app": "node index",
    "start": "concurrently \"npm run tsc:w\" \"npm run app\" ",
    "start:w": "concurrently \"npm run webpack\" \"npm run app\" ",
    "build": "webpack --config webpack.prod.js",
    "test:back": "NODE_ENV=test mocha --reporter spec app/tests",
    "test:genetics": "NODE_ENV=test DEBUG=genetics mocha --reporter spec app/tests/genetics"
  },
  "keywords": [
    "genetics"
  ],
  "author": "Brigitte Hofmeister",
  "license": "Apache-2.0",
